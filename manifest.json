{
    "manifest_version": 2,
    
    "name": "순간이동기",
    "description": "BOJ Move Tool",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "https://*/*",
        "input",
        "<all_urls>"
    ]
}